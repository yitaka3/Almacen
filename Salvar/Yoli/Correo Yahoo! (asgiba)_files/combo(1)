<xsl:template name="parseAndSort"><xsl:param name="whichFuncLib" /><xsl:variable name="copied_nodes"><xsl:for-each select="/ab/gr | /ab/ct"><xsl:copy-of select="." /></xsl:for-each></xsl:variable><xsl:variable name="modified_nodes"><xsl:choose><xsl:when test="$whichFuncLib = 1"><xsl:call-template name="parseNodes"><xsl:with-param name="node_list" select="msxsl:node-set($copied_nodes)/*" /></xsl:call-template></xsl:when><xsl:otherwise><xsl:call-template name="parseNodes"><xsl:with-param name="node_list" select="exsl:node-set($copied_nodes)/*" /></xsl:call-template></xsl:otherwise></xsl:choose></xsl:variable><xsl:choose><xsl:when test="$whichFuncLib = 1"><xsl:for-each select="msxsl:node-set($modified_nodes)/*"><xsl:sort select="@firstLastName" order="ascending"/><xsl:choose><xsl:when test="local-name(.) =\'gr\'"><xsl:variable name="group_name" select="./@fn" /><xsl:variable name="contact_count" select="count(msxsl:node-set($modified_nodes)/*[contains(@li, $group_name)])" /><xsl:if test="$contact_count &gt; 0"><xsl:copy-of select="." /></xsl:if></xsl:when><xsl:otherwise><xsl:copy-of select="." /></xsl:otherwise></xsl:choose></xsl:for-each></xsl:when><xsl:otherwise><xsl:for-each select="exsl:node-set($modified_nodes)/*"><xsl:sort select="@firstLastName" order="ascending"/><xsl:choose><xsl:when test="local-name(.) =\'gr\'"><xsl:variable name="group_name" select="./@fn" /><xsl:variable name="contact_count" select="count(exsl:node-set($modified_nodes)/*[contains(@li, $group_name)])" /><xsl:if test="$contact_count &gt; 0"><xsl:copy-of select="." /></xsl:if></xsl:when><xsl:otherwise><xsl:copy-of select="." /></xsl:otherwise></xsl:choose></xsl:for-each></xsl:otherwise></xsl:choose></xsl:template><xsl:template name="parseNodes"><xsl:param name="node_list" /><xsl:for-each select="$node_list"><xsl:choose><xsl:when test="local-name(.) = \'gr\'"><xsl:call-template name="copyGroup"><xsl:with-param name="item" select="." /></xsl:call-template></xsl:when><xsl:otherwise><xsl:call-temp